<launch>
	<node pkg="manyears_ros" type="manyears_ros" name="many_ears" output="screen">
		<param name="use_audio_stream" value="true"/>
		<param name="raw_file" value="$(find manyears_ros)/data/debord.raw" />
		<param name="config_file" value="$(find manyears_ros)/data/cob3-1.mes" />
		<param name="enable_separation" value="true" />
        <param name="instant_time" value="true" />
	</node>
</launch>
